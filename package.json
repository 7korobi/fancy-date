{
  "name": "vue-petit-store",
  "version": "0.2.0",
  "description": "petit store for vue mixin",
  "main": "lib/index.min.js",
  "repository": "git@github.com:7korobi/vue-petit-store.git",
  "homepage": "https://github.com/7korobi/vue-petit-store",
  "bugs": {
    "url": "https://github.com/7korobi/vue-petit-store/issues"
  },
  "author": "7korobi",
  "license": "MIT",
  "keywords": [
    "store",
    "vuex",
    "cookie",
    "localstorage",
    "sessionstorage",
    "href"
  ],
  "tags": [
    "store",
    "vuex",
    "cookie",
    "localstorage",
    "sessionstorage",
    "href"
  ],
  "preferGlobal": true,
  "private": false,
  "jest": {
    "testRegex": "/__tests__/.*\\-spec.coffee$",
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "transform": {
      "^.+\\.coffee$": "<rootDir>/config/coffee-jest.js",
      "^.+\\.js$": "babel-jest",
      "^.+\\.pug$": "pug-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@nuxt/babel-preset-app": "^2.4.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.2",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "pug-jest": "^1.0.1",
    "tiny-cookie": "^2.3.1",
    "vue": "^2.6.7",
    "vue-jest": "^3.0.3",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.7",
    "vuex": "^3.1.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "test": "run-s make jest",
    "make": "run-p make:*",
    "make:index": "webpack --config config/webpack-index.coffee",
    "jest": "jest -u",
    "bench": "node test --bench",
    "preversion": "npm run build && (git diff --quiet || git commit -am 'minify')",
    "g": "run-p -n g:*",
    "g:npm": "npm install -g npm-check-updates"
  },
  "dependencies": {
    "firebase": "^5.8.4",
    "memory-orm": "^0.2.0"
  }
}
